<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFrameworks>netcoreapp31;net48;net462</TargetFrameworks>
    <AssemblyName>JdUtils</AssemblyName>
    <RootNamespace>JdUtils</RootNamespace>
    <Authors>Jan Duchacek</Authors>
    <Company>JGS</Company>
    <Product>JdUtils</Product>
    <Description>Utilities for wpf</Description>
    <Copyright>Jan Duchacek</Copyright>
    <PackageProjectUrl>https://github.com/duchacekjan/JdUtils</PackageProjectUrl>
    <RepositoryUrl>https://github.com/duchacekjan/JdUtils.git</RepositoryUrl>
    <PackageTags />
    <PackageReleaseNotes>Extended converters and markups for constructor s definiton of default value. Created base converter for converters with negate property. Breaking changes: StringNullOrEmptyConverter property Inverse renamed to Negate</PackageReleaseNotes>
    <RepositoryType>git</RepositoryType>
    <PackageIcon>jdUtilsIcon.png</PackageIcon>
    <PackageIconUrl />
    <Version>1.0.9</Version>
    <UseWPF>true</UseWPF>
  </PropertyGroup>
  <ItemGroup>
    <None Include="jdUtilsIcon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Resources\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Resources\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>
</Project>